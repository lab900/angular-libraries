{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "angular-libraries": [
        "dist/angular-libraries/angular-libraries",
        "dist/angular-libraries"
      ],
      "forms": [
        "dist/forms/forms",
        "dist/forms"
      ],
      "ui": [
        "dist/ui/ui",
        "dist/ui"
      ],
      "admin": [
        "dist/admin/admin",
        "dist/admin"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
